<% if signed_in? %>
	<% if @user.id == 1 %>
		<div class="row">
			<aside class="span4">
				<% @labomem.where(:labo => @user.labo).find_each do |labo| %>
				<% unless labo.id == 1 %>
					<% text = link_to labo.name, controller: "static_pages", action: "home",labomem: labo.id %>
					
					<%= labo.student_id %>
					<%= text %>
					<p></P>
				<% end %>
				<% end %>
			</aside>
			<% if params[:labomem] %>
				<% sum = 0.0 %>
				<% @calendar_all.where(:user_id => params[:labomem]).find_each do |count| %>
					<% sum += count.time.to_f %>
				<% end %>
				<section>
					合計 <%= number_with_precision(sum, :precision => 1) %> 時間
				</section>
				<%= link_to "11", controller: "static_pages", action: "home", month: 11,labomem: params[:labomem] %>
				<%= link_to "12", controller: "static_pages", action: "home", month: 12,labomem: params[:labomem] %>
				<p></p>
				<% if params[:month] %>
					<% @calendar_all.where(:user_id => params[:labomem]).find_each do |calendar| %>
						<% unless calendar.time == nil %>
							<% if calendar.date[4,2] == params[:month] %>
								<%= link_to calendar.date[6,2], controller: "calendars", action: "index",date: calendar.date,id: calendar.user_id %>
								<%= calendar.time %>
								<p></p>
							<% end %>
						<% end %>
					<% end %>
				<% end %>
			<% end %>
		</div>
	<% else %>
	<div class="row">
		<aside class="span4">
